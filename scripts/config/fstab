LABEL=ARCH_BOOT /efi vfat rw,relatime,fmask=0022,dmask=0022,codepage=437,iocharset=ascii,shortname=mixed,utf8,nofail,,x-systemd.device-timeout=3,errors=remount-ro 0 2
